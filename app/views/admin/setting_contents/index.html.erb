<h1>Listing setting_contents</h1>

<table>
  <thead>
    <tr>
      <th>Terms condition</th>
      <th>About us</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @setting_contents.each do |setting_content| %>
      <tr>
        <td><%= setting_content.terms_condition %></td>
        <td><%= setting_content.about_us %></td>
        <td><%#= link_to 'Show', admin_setting_content(setting_content) %></td>
        <td><%#= link_to 'Edit', edit_setting_content_path(setting_content) %></td>
        <td><%#= link_to 'Destroy', setting_content, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

