<h1>Listing business_businesses</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @business_businesses.each do |business_business| %>
      <tr>
        <td><%= link_to 'Show', business_business %></td>
        <td><%= link_to 'Edit', edit_business_business_path(business_business) %></td>
        <td><%= link_to 'Destroy', business_business, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Business', new_business_business_path %>
